	<subDataset name="PO_NO" uuid="403c05d3-db4b-4666-b0c6-84ef78ac0145">
		<parameter name="p_no" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select  IND_NO
from terms.po_detail_temp,TERMS.PO_DETAIL PD
where PO_NO=$P{p_no}
and ITEM_CD=PD.PIOS_ITEM_CD]]>
		</queryString>
		<field name="IND_NO" class="java.lang.String"/>
	</subDataset>
	<parameter name="p_no" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="p_unit" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[WITH UNIT_TEMP AS (select LOGO_PATH from terms.unit where rownum = 1 order by code)
SELECT rownum sr,
        CANC_FLAG,
       PIOS_ITEM_CD,
       PD.DISCOUNT_PERCENT,
       PH.JO_PO,
       Decode(PH.JO_PO,'J','JOBWORK ORDER','PURCHASE ORDER')as PO_TYPE,
       pios_pois_no,
       (select  LISTAGG(IND_NO,',') within group(order by IND_NO)
       from terms.po_detail_temp
       where PO_NO=PD.PO_HEAD_PO_NO
       and ITEM_CD=PD.PIOS_ITEM_CD
       and AMD_NO= ph.AMD_NO ) IND_NO,
       NAME,
       Address,
       po_status,
       PH.UNIT_CD,
       PH.APPR_BY,
       city_code,
       case when email is null then ' ' else  'E-mail : '||email end email, -----1------
       ven_cd,
       (select distinct pan_no from terms.vendor_master where vendor_code=ven_cd) pan_no,
       (select distinct name from terms.vendor_master where vendor_code=ven_cd) ven_name,
       PRICE_BASIS_REMARK,
       PH.REMARKS HEAD_REMARK,
       UNLOAD_ADDRESS,
       PD.ED_DATE,
       (select distinct GST_REG_NO from terms.vendor_master where vendor_code=ven_cd) ven_gst_no,
       (select distinct EMP_FIRST_NAME||' '||EMP_LAST_NAME from terms.emp_master where emp_number=PREP_BY) contact_name,
       (select distinct COMPANY_EMAIL from terms.emp_master where emp_number=PREP_BY) contact_Email,
       VMAIL,
       (select ADDRESS1||ADDRESS3||ADDRESS3 name from TERMS.VENDOR_REGD_ADDRESS where vendor_code=ven_cd) address1,
       PO_HEAD_PO_NO,
       (select ITEM_DESC FROM TERMS.ITEM_STOCK WHERE ITEM_CD=PD.PIOS_ITEM_CD
       union
        select ITEM_DESC from TERMS.inv_ser_bill_view where ITEM_CD =PD.PIOS_ITEM_CD ) DESCS,
       PO_HSN_CODE,
       QTY,
       UOM,
       MATERIAL_RATE,
       DISCOUNT_AMT,
       discount_percent,

       CGST_PER,
       SGST_PER,
       pD.REMARKS,
       GST_AMT,
       GOODS_VALUE,
       (SELECT DESCRIPTION FROM  TERMS.SEC_CONTROL_VALUES WHERE CONTROL_CODE=PO_TYPE AND CONTROL_TYPE='PO_TYPE')HEADING,
       decode(FREIGHT,0, null,FREIGHT)FREIGHT,
       decode(INS_AMT,0,null,INS_AMT)INS_AMT,
       SGST_AMT,
       CGST_AMT,
       (MATERIAL_RATE*QTY)BASIC,
       (SELECT distinct GST_REG_NO FROM TERMS.UNIT WHERE CODE=UNIT_CD)GST,
       (select DISTINCT STATE_GST_CODE from TERMS.VENDOR_REGD_ADDRESS where vendor_code=ven_cd)STATECODE,
       (SELECT DISTINCT PHONES FROM TERMS.VENDOR_REGD_ADDRESS WHERE VENDOR_CODE=VEN_CD) PHONES,
       (select DISTINCT lower(telex) from TERMS.VENDOR_REGD_ADDRESS where vendor_code=ven_cd) e_mail,
       (select DISTINCT state from TERMS.VENDOR_REGD_ADDRESS where vendor_code=ven_cd)  StateName,
       (select DISTINCT city from TERMS.VENDOR_REGD_ADDRESS where vendor_code=ven_cd)  CityNam,
       (SELECT DISTINCT PIN_CODE FROM TERMS.VENDOR_REGD_ADDRESS WHERE vendor_code = ven_cd) VENDOR_PIN,
        ITEM_SPECF,
       (select PERMANENT_MOB  from terms.emp_master where emp_number=PREP_BY)PHN,
       (select PERMANENT_PHONE  from TERMS.EMP_MASTER where EMP_NUMBER = PREP_BY)emp_phone,
       PH.PO_DT,
       GST_PERCENT,
      -- NAME,
       pd.po_id,
      ((nvl(GST_AMT,0)+nvl(SGST_AMT,0)+nvl(CGST_AMT,0))*nvl(FREIGHT,1))/100 fright_amt,
      ph.amd_no,
      ph.amd_dt,
      decode(ut.SBU_CODE, 101, 'Y', 'N') SBU_FLG,
      SUM(NVL((((NVL(QTY,0))*(NVL(MATERIAL_RATE,0))))-(NVL(DISCOUNT_AMT,0)),0)) OVER (ORDER BY PD.PO_HEAD_PO_NO)
      +sum(nvl(GST_AMT,0))OVER (ORDER BY PD.PO_HEAD_PO_NO)
      +sum(nvl(SGST_AMT,0))OVER (ORDER BY PD.PO_HEAD_PO_NO)
      +sum(nvl(CGST_AMT,0))OVER (ORDER BY PD.PO_HEAD_PO_NO)
      +(((sum(nvl(GST_AMT,0))OVER (ORDER BY PD.PO_HEAD_PO_NO)
      +sum(nvl(SGST_AMT,0))OVER (ORDER BY PD.PO_HEAD_PO_NO)
      +sum(nvl(CGST_AMT,0))OVER (ORDER BY PD.PO_HEAD_PO_NO))*nvl(FREIGHT,0))/100)+nvl(INS_AMT,0) NET_AMT,
      (SELECT ENAME FROM TERMS.EMP_VIEW WHERE ENO = PH.CHCK_BY and unit = PH.UNIT_CD) VERIFIED_NM,
      (select ENAME from TERMS.EMP_VIEW where ENO = PH.FREZ_BY and unit = PH.UNIT_CD) APPR_NM,
       CASE WHEN UT.CIN IS NULL THEN ' ' ELSE 'CIN No : '||UT.CIN END CIN, ------2----------
      (SELECT LOGO_PATH FROM TERMS.UNIT_TEMP) LOGO_PATH,
      (SELECT DESCRIPTION FROM TERMS.SEC_CONTROL_VALUES A WHERE PH.DEL_TERM = A.CONTROL_CODE AND A.APPLICATION_CODE = 'MTL') DEL_TERMS,
      PH.CUR_CD,
 (SELECT EMP_FIRST_NAME||' '||EMP_LAST_NAME FROM  TERMS.EMP_MASTER WHERE  EMP_NUMBER=ph.PREP_BY) prepared_by,
                (SELECT EMP_FIRST_NAME||' '||EMP_LAST_NAME FROM  terms.EMP_MASTER WHERE  EMP_NUMBER =ph.APPR_BY ) authorised_by,
(SELECT EMP_FIRST_NAME||' '||EMP_LAST_NAME FROM  terms.EMP_MASTER WHERE  EMP_NUMBER =PH.CHCK_BY )VERIFIED_by,
     INITCAP( TERMS.AMOUNT_TO_WORD((round(SUM(NVL((((NVL(QTY,0))*(NVL(MATERIAL_RATE,0))))-(NVL(DISCOUNT_AMT,0)),0)) OVER (ORDER BY PD.PO_HEAD_PO_NO),2)
       +sum(nvl(GST_AMT,0))OVER (ORDER BY PD.PO_HEAD_PO_NO)
      + ROUND(sum(nvl(SGST_AMT,0))OVER (ORDER BY PD.PO_HEAD_PO_NO),2)
      + ROUND(sum(nvl(CGST_AMT,0))OVER (ORDER BY PD.PO_HEAD_PO_NO),2)
      +(((sum(nvl(GST_AMT,0))OVER (ORDER BY PD.PO_HEAD_PO_NO)
      + ROUND (sum(nvl(SGST_AMT,0))OVER (ORDER BY PD.PO_HEAD_PO_NO),2)
      + ROUND(sum(nvl(CGST_AMT,0))OVER (ORDER BY PD.PO_HEAD_PO_NO),2))*nvl(FREIGHT,0))/100)+nvl(INS_AMT,0)), PH.CUR_CD)) amt_wrd,

case when nvl(Instr(UT.ADDRESS, 'Licence'),0)>0
     then Substr(UT.ADDRESS, 1, Instr(UT.ADDRESS, 'Licence')-2)
     else UT.ADDRESS
end unit_ADDRESS,
case when nvl(Instr(UT.ADDRESS, 'Licence'),0)>0
     then --Substr(UT.ADDRESS, 1, Instr(UT.ADDRESS, 'Licence')-2)
     Substr(UT.ADDRESS, Instr(UT.ADDRESS, 'Licence'),length(UT.ADDRESS))
     else ' '
end UNIT_ADD


FROM TERMS.PO_DETAIL PD,
     TERMS.PO_HEAD PH,
     TERMS.UNIT UT

WHERE PD.PO_HEAD_PO_NO = PH.PO_NO
  and ph.AMD_NO        = pd.PO_HEAD_AMD_NO
  AND PH.UNIT_CD       = CODE
  AND PH.AMD_NO        = (SELECT MAX(AMD_NO) FROM TERMS.PO_HEAD A WHERE A.PO_NO=PH.PO_NO)
  AND PD.PO_HEAD_PO_NO = $P{p_no}
  and UNIT_CD          = $P{p_unit}]]>
	</queryString>
