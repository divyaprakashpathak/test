		<parameter name="p_unit" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT NVL(FREIGHT,0)+NVL(INS_AMT,0)+(NVL((SELECT SUM(NVL(MATERIAL_RATE,0))*SUM(NVL(QTY,0))+SUM(NVL(GST_AMT,0))+SUM(NVL(SGST_AMT,0))+SUM(NVL(CGST_AMT,0))
FROM terms.PO_DETAIL
 WHERE PO_HEAD_PO_NO=PO_NO AND PO_HEAD_AMD_NO=AMD_NO AND UNIT_CD=UNIT_CD),0)) MATER FROM PO_HEAD
 WHERE PO_NO=$P{p_no}
and AMD_NO=(select max(amd_no) from po_head a where a.po_no=$P{p_no})
and UNIT_CD=$P{p_unit}]]>
		</queryString>
		<field name="MATER" class="java.math.BigDecimal">
			<fieldDescription><![CDATA[]]></fieldDescription>
		</field>
	</subDataset>
