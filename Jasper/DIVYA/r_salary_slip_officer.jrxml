	<subDataset name="DEDUCTIONS" uuid="c53f003a-0638-46f6-9b47-e0c4b39298c3">
		<parameter name="p_emp_no" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="p_unit" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="p_month" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="p_year" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[with EMP_SAL as (
SELECT
    EMPERNDED_EMP_NO
    ,AMOUNT
    ,UNIT_CD
    ,EMPERNDED_HEAD_CODE
    ,DECODE(LENGTH(EMPERNDED_HEAD_CODE),2,('0'||EMPERNDED_HEAD_CODE),1,('00'||EMPERNDED_HEAD_CODE),EMPERNDED_HEAD_CODE) HEAD_CODE
    ,EMPERNDED_HEAD_TYPE
    ,PAYMENT_FOR_MONTH
FROM
    TERMS.EMP_SAL_MASTER
),

SAL_HD AS(
SELECT
    CODE
    ,DECODE(LENGTH(CODE),2,('0'||CODE),1,('00'||CODE),CODE) HEAD_CODE
    ,SHORT_DESC
    ,UNIT_CD
FROM TERMS.SAL_HD_MAST
)


SELECT DISTINCT
    round(es.AMOUNT,0)AMOUNT
    ,sh.short_desc DESCRIPTION,sh.head_code
 FROM EMP_SAL ES,SAL_HD SH
WHERE es.unit_cd                	=       	sh.unit_cd
  AND es.head_code              	=       	sh.head_code
  AND es.empernded_head_type    	=       	'D'
  AND es.empernded_emp_no       	=       	$P{p_emp_no}
  AND es.unit_cd                	=       	$P{p_unit}
  AND TO_CHAR(PAYMENT_FOR_MONTH,'MON') 	= 	$P{p_month}
  AND TO_CHAR(PAYMENT_FOR_MONTH,'YYYY')	=	$P{p_year}
ORDER BY sh.head_code]]>
		</queryString>
		<field name="AMOUNT" class="java.math.BigDecimal"/>
		<field name="DESCRIPTION" class="java.lang.String"/>
		<field name="HEAD_CODE" class="java.lang.String"/>
		<variable name="E_Total" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$F{AMOUNT}]]></variableExpression>
		</variable>
	</subDataset>
