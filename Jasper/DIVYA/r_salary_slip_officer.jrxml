	<queryString>
		<![CDATA[select      Distinct
            a.empernded_emp_no,
            a.pf_salary,
            a.esi_salary,
            a.empernded_head_type,
            a.empernded_head_code,
            a.amount,
            a.emp_desig_cd,
            a.emp_catg,
            a.unit_cd,
            trim(to_char(a.payment_for_month,'MON')) MON,
            trim(to_char(a.payment_for_month,'YYYY')) YR,
            trim(to_char(a.payment_for_month,'Month'))||' - '||trim(to_char(a.payment_for_month,'YYYY')) payment_for_month,
            trim(b.emp_first_name)||' '||trim(b.emp_last_name)emp_name,
            b.father_name,
            b.card_no,
            b.division_description,
            b.pf_number pf_code,
            b.esi_number,
            b.bank_acc_number,
            b.uan_no,
            b.pan_no,
            b.adhaar_no,
            b.join_date,
            (select bm.name from terms.hrm_bank_master bm where bm.code = b.bankmast_code)bank_name,
            (select bm.ifsc_cd from terms.hrm_bank_master bm where bm.code = b.bankmast_code)ifsc,
            (select description from terms.hrm_location_master lm where lm.code = b.area_mast_code and lm.unit_cd=b.unit)location,
            (select name from terms.department dt where dt.code = a.emp_dept_cd and dt.unit_code = a.unit_cd)department,
            nvl((select sum(nvl(amount,0))from terms.emp_sal_master em where em.empernded_emp_no = a.empernded_emp_no and em.unit_cd = a.unit_cd and em.empernded_head_code='001'),0)basic,
            round(nvl((select sum(nvl(amount,0))from terms.emp_sal_master em where em.empernded_emp_no = a.empernded_emp_no and em.unit_cd = a.unit_cd and em.empernded_head_type='E' and to_char(em.payment_for_month,'MON')=to_char(a.payment_for_month,'MON') and to_char(em.payment_for_month,'YYYY')=to_char(a.payment_for_month,'YYYY')),0))total_ear,
            round(nvl((select sum(nvl(amount,0))from terms.emp_sal_master em where em.empernded_emp_no = a.empernded_emp_no and em.unit_cd = a.unit_cd and em.empernded_head_type='D' and to_char(em.payment_for_month,'MON')=to_char(a.payment_for_month,'MON') and to_char(em.payment_for_month,'YYYY')=to_char(a.payment_for_month,'YYYY')),0))total_ded,
            round(nvl((select sum(nvl(decode(em.empernded_head_type,'E',em.amount),0))-sum(nvl(decode(em.empernded_head_type,'D',em.amount),0))from terms.emp_sal_master em where em.empernded_emp_no = a.empernded_emp_no and em.unit_cd = a.unit_cd),0))net,
            initcap(terms.amount_to_word(round(nvl((select sum(nvl(decode(em.empernded_head_type,'E',em.amount),0))-sum(nvl(decode(em.empernded_head_type,'D',em.amount),0))from terms.emp_sal_master em where em.empernded_emp_no = a.empernded_emp_no and em.unit_cd = a.unit_cd),0)),'INR'))net_in_word,
            u.name,
            case
                when nvl(Instr(U.ADDRESS, 'Licence'),0)>0
                then substr(Substr(U.ADDRESS, 1, Instr(U.ADDRESS, 'Licence')-2),1,length(Substr(U.ADDRESS, 1, Instr(U.ADDRESS, 'Licence')-2))-1)
                else U.ADDRESS
            end address,
            u.logo_path logo,
            (select description from terms.sal_hd_mast sm where sm.code = a.empernded_head_code and sm.unit_cd = a.unit_cd)ern_dedcn_desc,
            (select description from terms.designation dg where dg.code = a.emp_desig_cd and dg.unit_cd = a.unit_cd)designation_desc,
            nvl((select wd.work_days from terms.working_days wd where wd.unit_cd = a.unit_cd and wd.month = to_char(a.payment_for_month,'MON') and wd.year = to_char(a.payment_for_month,'YYYY')),0)no_working,
            nvl((select am.no_of_days from terms.attend_master am where am.emp_no = a.empernded_emp_no and am.unit_code = a.unit_cd and am.month = to_char(a.payment_for_month,'MON') and am.year = to_char(a.payment_for_month,'YYYY') and am.attend_leave_type='AB'),0)abs_days,
            sum(nvl(decode(empernded_head_type,'E',a.amount),0))over(partition by a.empernded_emp_no,a.empernded_head_code)ear,
            sum(nvl(decode(empernded_head_type,'D',a.amount),0))over(partition by a.empernded_emp_no,a.empernded_head_code)ded,
            nvl((select sum(eh.amount) from terms.emp_earn_dedn_history eh where eh.emp_no = a.empernded_emp_no and to_char(a.payment_for_month,'MON-YYYY') = to_char(eh.trans_date,'MON-YYYY') and head_code in ('001','003','118','018','016','912','619')),0) gross_total,
            nvl((select sum(eh.amount) from terms.emp_earn_dedn_history eh where eh.emp_no = a.empernded_emp_no and to_char(a.payment_for_month,'MON-YYYY') = to_char(eh.trans_date,'MON-YYYY') and head_code in ('001')),0)basic1,
            nvl((select sum(eh.amount) from terms.emp_earn_dedn_history eh where eh.emp_no = a.empernded_emp_no and to_char(a.payment_for_month,'MON-YYYY') = to_char(eh.trans_date,'MON-YYYY') and head_code in ('003')),0)HRA,
            nvl((select sum(eh.amount) from terms.emp_earn_dedn_history eh where eh.emp_no = a.empernded_emp_no and to_char(a.payment_for_month,'MON-YYYY') = to_char(eh.trans_date,'MON-YYYY') and head_code in ('018')),0)other_allow,
            nvl((select sum(eh.amount) from terms.emp_earn_dedn_history eh where eh.emp_no = a.empernded_emp_no and to_char(a.payment_for_month,'MON-YYYY') = to_char(eh.trans_date,'MON-YYYY') and head_code in ('118')),0)conveyance,
            nvl((select sum(eh.amount) from terms.emp_earn_dedn_history eh where eh.emp_no = a.empernded_emp_no and to_char(a.payment_for_month,'MON-YYYY') = to_char(eh.trans_date,'MON-YYYY') and head_code in ('016')),0)cca,
            nvl((select sum(eh.amount) from terms.emp_earn_dedn_history eh where eh.emp_no = a.empernded_emp_no and to_char(a.payment_for_month,'MON-YYYY') = to_char(eh.trans_date,'MON-YYYY') and head_code in ('912')),0)medical_alow,
            nvl((select sum(eh.amount) from terms.emp_earn_dedn_history eh where eh.emp_no = a.empernded_emp_no and to_char(a.payment_for_month,'MON-YYYY') = to_char(eh.trans_date,'MON-YYYY') and head_code in ('619')),0)spcl,

            (select at.total_paid_days from terms.adv_att at where at.emp_no = a.empernded_emp_no and at.unit_cd = a.unit_cd and at.mon||'-'||at.year = to_char(a.payment_for_month,'MON-YYYY'))total_payble_days,
            nvl((select sum(ad.amount) from terms.arrear_detail ad where ad.emp_no = a.empernded_emp_no and ad.salhd_mast_type = 'E' and ad.sal_month||'-'||ad.sal_year = to_char(a.payment_for_month,'MON-YYYY')),0)Earning_arrear,
            nvl((select sum(ad.amount) from terms.arrear_detail ad where ad.emp_no = a.empernded_emp_no and ad.sal_hd_code = '007' and ad.sal_month||'-'||ad.sal_year = to_char(a.payment_for_month,'MON-YYYY')),0)deduction_arrear

from        terms.emp_sal_master a
left join   terms.emp_master_hd b   on a.empernded_emp_no = b.emp_number
                                    and a.unit_cd = b.unit
left join   terms.unit u            on a.unit_cd = u.code

where       a.unit_cd = $P{p_unit}
and         a.empernded_emp_no like $P{p_emp_cd}||'%'
and         to_char(a.payment_for_month,'MON')=$P{p_month}
and         to_char(a.payment_for_month,'YYYY')=$P{p_year}
and         b.division_code like $P{p_div}||'%'
and         b.card_no like $P{p_card}||'%'

order by    a.empernded_emp_no]]>
	</queryString>
