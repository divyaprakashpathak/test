	<queryString>
		<![CDATA[select * from
(SELECT 1 GR, rownum sr, A.DRIVER_NAME, A.DRIVER_CONTACT, A.DOC_NO, C.NAME, A.DEL_MODE, A.VEHICLE_NO, A.CHALLAN_NO
     , A.CHALLAN_DT, A.DOC_DT, A.TRPT_CD, A.FRM_LOC FRM_LOC_CD, A.SHIP_TO SHIP_TO_CD, A.EWAY_NO, A.EWAY_DATE
     , (SELECT LOGO_PATH FROM TERMS.UNIT WHERE UNIT_HO = 'HO' AND SBU_CODE = (SELECT SBU_CODE FROM TERMS.UNIT WHERE CODE = A.UNIT_CD)) LOGO_PATH
     , (SELECT ADDRESS FROM TERMS.LOCATION WHERE LOCAT_CODE=FRM_LOC)||FRM_LOC AS FROM_LOC
     , (SELECT ADDRESS FROM TERMS.LOCATION WHERE LOCAT_CODE=TO_LOC)|| TO_LOC AS TO_LOC
     , (SELECT NAME FROM TERMS.VENDOR_MASTER WHERE VENDOR_CODE=TRPT_CD) AS VENDOR_NAME
     , (SELECT ADDRESS FROM TERMS.LOCATION WHERE LOCAT_CODE= FRM_LOC) AS FROM_LOC_ADD
     , (SELECT ADDRESS FROM TERMS.LOCATION WHERE LOCAT_CODE= TO_LOC) AS TO_LOC_ADD
     , B.MRP, (SELECT STATE FROM TERMS.VENDOR_REGD_ADDRESS WHERE VENDOR_CODE=TRPT_CD) AS STATE
     , B.AMT as amount, (select name from terms.unit where code=A.UNIT_CD) as uni_name
     , B.ITEM_CD, B.DESCRIPT AS ITEM_DESC, B.HSN_NO, B.BATCH_NO, B.EXP_DT, B.MFG_DT, B.QTY
     , sum(B.QTY) over(partition by B.ITEM_CD)SUM_QTY, B.QTY * 100 / 1000 as qty_kg_lr
     , D.CONVER_FACT
     , CASE WHEN D.CONVER_FACT = 0 THEN 0 ELSE CEIL(B.QTY/D.CONVER_FACT) END AS PACKING_QTY
     , Count(1)over(partition by B.ITEM_CD order By B.Batch_no) item_count
     , CASE WHEN D.CONVER_FACT = 0 THEN 0 ELSE CEIL(sum(B.QTY)over(partition by B.ITEM_CD)/D.CONVER_FACT)
          * (Case When (Count(1) over (partition by B.ITEM_CD) = Count(1)over(partition by B.ITEM_CD order By B.Batch_no))
                then 1 else null end)  END AS PACKING_QTY_2
     , SUM(ROUND_CLD)OVER (PARTITION BY B.ITEM_CD) PACKING_QTY_3
--     , CASE WHEN D.CONVER_FACT = 0 THEN 0 ELSE CEIL(sum(B.QTY)over(partition by B.ITEM_CD)/D.CONVER_FACT) END AS PACKING_QTY_3
     , count(B.ITEM_CD)over(partition by B.ITEM_CD)no_of_item, B.UOM, B.RATE, TO_NUMBER(B.PACKAGE_DETAILS)PACKAGE_DETAILS
     , C.GST_REG_NO, C.PAN_NO, IS_D.PROC_CD, PROC.LONG_DESCRIP AS PROC_DSC
     , INITCAP( TERMS.AMOUNT_TO_WORD(sum(B.AMT) over (partition by A.CHALLAN_NO), 'INR')) INWORDS
     , D.UOM ALT_UOM
     , (select DESCRIPTION from terms.product where CODE=B.ITEM_CD
	 union all
	select DESCRIPTION from terms.part_master where PART_CODE=B.ITEM_CD) p_description
     , A.REMARKS, A.AUTH_BY, decode (A.AUTH_BY,'','UNAPPROVED')Status
     , (SELECT CASE WHEN PART_TYP ='SF' THEN B.QTY ELSE NVL((B.QTY * (SELECT CUT_WT FROM TERMS.PRODUCTS WHERE PRODUCT_CODE=B.ITEM_CD)),B.QTY ) END
          FROM (SELECT PART_TYP FROM TERMS.PART_MASTER WHERE PART_CODE=B.ITEM_CD UNION SELECT STATUS FROM TERMS.PRODUCT WHERE CODE=B.ITEM_CD)) AS QTY_NEW
     , TO_CHAR(A.LAST_UPDATE_DATE,'HH:MI AM') AS CREATION_TIME
  FROM TERMS.CCGP_HEAD A
     , TERMS.CCGP_DETLS B
     , TERMS.UNIT C
     , TERMS.ITEM_PACKING_UOM D
     , TERMS.INVOICE_HEADER INV_H
     , TERMS.iss_dslip IS_D
     , TERMS.PROCESS PROC
 WHERE A.CHALLAN_NO = B.CCGP_HEAD_CHALLAN_NO
   AND A.UNIT_CD    = C.CODE
   AND D.ITEM_CD(+) = B.ITEM_CD
   AND D.UNIT_CD(+) = A.UNIT_CD
   AND B.DOC_NO =  INV_H.IDENTIFIER(+)
   AND A.CHALLAN_NO =INV_H.CHALLAN_ISSUED (+)
   AND INV_H.CCGP_NO = IS_D.ISS_HSLIP_SLIP_NO(+)
   AND B.ITEM_CD = IS_D.ITEM_CD(+)
   AND IS_D.PROC_CD = PROC.PROC_CODE(+)
   AND A.CHALLAN_NO=$P{p_no}
   AND A.UNIT_CD=$P{p_unit}
)order by ITEM_CD,item_count desc]]>
	</queryString>
