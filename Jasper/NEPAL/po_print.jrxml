	<subDataset name="New Dataset 1" uuid="0a490fc2-1c2e-45fb-8be5-a03171df948b">
		<parameter name="p_no" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="p_unit" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT NVL(FREIGHT,0)+NVL(INS_AMT,0)+(NVL((SELECT SUM(NVL(MATERIAL_RATE,0))*SUM(NVL(QTY,0))+SUM(NVL(GST_AMT,0))+SUM(NVL(SGST_AMT,0))+SUM(NVL(CGST_AMT,0))
FROM terms.PO_DETAIL
 WHERE PO_HEAD_PO_NO=PO_NO AND PO_HEAD_AMD_NO=AMD_NO AND UNIT_CD=UNIT_CD),0)) MATER FROM terms.PO_HEAD
 WHERE PO_NO=$P{p_no}
and AMD_NO=(select max(amd_no) from terms.po_head a where a.po_no=$P{p_no})
and UNIT_CD=$P{p_unit}]]>
		</queryString>
		<field name="MATER" class="java.math.BigDecimal"/>
	</subDataset>
	<subDataset name="New Dataset 2" uuid="9abdfe1f-a4a1-4d3b-8970-f87639e94633">
		<parameter name="p_no" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select SUBJECT from  terms.ANNEX_PO_MST
where PO_NO=$P{p_no}
and AMD_NO=(select max(amd_no) from terms.po_head a where a.po_no=$P{p_no})]]>
		</queryString>
		<field name="SUBJECT" class="java.lang.String"/>
	</subDataset>
	<parameter name="p_no" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="p_unit" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT PIOS_ITEM_CD,
       ven_cd,
       NAME,
       ADDRESS,
       TELEPHONE,
       PAN_NO,
       (select distinct name from terms.vendor_master where vendor_code=ven_cd) ven_name,
       (select distinct GST_REG_NO from terms.vendor_master where vendor_code=ven_cd) ven_gst_no,
       (select distinct EMP_FIRST_NAME||' '||EMP_LAST_NAME from terms.emp_master where emp_number=FREZ_BY) contact_name,
       (select distinct COMPANY_EMAIL from terms.emp_master where emp_number=FREZ_BY) contact_Email,
       (select ADDRESS1||ADDRESS2||ADDRESS3 name from terms.VENDOR_REGD_ADDRESS where vendor_code=ven_cd) vendaddress,
       PO_HEAD_PO_NO,
       (select ITEM_DESC from terms.ITEM_STOCK WHERE ITEM_CD=PD.PIOS_ITEM_CD) DESCS,
       PO_HSN_CODE,
       QTY,
       UOM,
       MATERIAL_RATE,
       DISCOUNT_AMT,
       CGST_PER,
       SGST_PER,
       GST_AMT,
       GOODS_VALUE,
       (SELECT DESCRIPTION from  TERMS.SEC_CONTROL_VALUES WHERE CONTROL_CODE=PO_TYPE AND CONTROL_TYPE='PO_TYPE')HEADING,
       FREIGHT,
       INS_AMT,
       SGST_AMT,
       CGST_AMT,
       (MATERIAL_RATE*QTY)BASIC,
       (SELECT distinct GST_REG_NO from terms.UNIT WHERE CODE=UNIT_CD)GST,
       (select DISTINCT STATE_GST_CODE from terms.VENDOR_REGD_ADDRESS where vendor_code=ven_cd)STATECODE,
       (select DISTINCT PHONES from terms.VENDOR_REGD_ADDRESS where vendor_code=ven_cd)PHONES,
       (select PRESENT_PHONE  from terms.emp_master where emp_number=FREZ_BY)PHN,PH.PO_DT,
       (SELECT 'Corporate office : '||ADDRESS from terms.company )corp_add,
       (SELECT CONTACT_PERSON from terms.VENDOR_REGD_ADDRESS where vendor_code=ven_cd)CNTACTPERSON,
       (select distinct EMP_FIRST_NAME||' '||EMP_LAST_NAME from terms.emp_master where emp_number=PREP_BY)prpby,
       (select distinct EMP_FIRST_NAME||' '||EMP_LAST_NAME from terms.emp_master where emp_number=chck_BY)chckby,
       (select distinct EMP_FIRST_NAME||' '||EMP_LAST_NAME from terms.emp_master where emp_number=APPR_BY)apprby,
       (select distinct EMP_FIRST_NAME||' '||EMP_LAST_NAME from terms.emp_master where emp_number=frez_by)authoby,
       pd.PO_HEAD_AMD_NO,
       PD.UC_AMOUNT,
       PD.GST_PERCENT,
       PD.GST_AMT,
       PD.SGST_PER,
       PD.SGST_AMT,
       (select SUBJECT from terms.ANNEX_PO_MST where PO_NO=PD.PO_HEAD_PO_NO and AMD_NO=pd.PO_HEAD_AMD_NO and rownum <=1)mst,
       CUR_CD

from terms.PO_DETAIL PD,
     terms.PO_HEAD PH,
     terms.UNIT UT

WHERE PD.PO_HEAD_PO_NO = PH.PO_NO
  AND ph.AMD_NO        = pd.PO_HEAD_AMD_NO
  AND PH.UNIT_CD       = UT.CODE
  AND PD.PO_HEAD_PO_NO = $P{p_no}
  AND ph.AMD_NO        = (select max(amd_no) from terms.po_head a where a.po_no=ph.po_no)
  AND UNIT_CD          = $P{p_unit}]]>
	</queryString>
