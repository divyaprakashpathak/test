	<subDataset name="AMT_WORD" uuid="42543e66-ed6b-47b7-a7fd-7c9fc83fd752">
		<parameter name="P_INV" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT 'Total Amount (In Words ) '||initcap(terms.AMOUNT_TO_WORD(gr_amt,'INR')) Net_amt_word,'Amount (In Words ) '||initcap(terms.AMOUNT_TO_WORD(tax_AMT,'INR')) Tax_amt_word FROM (
select sum(nvl(net_amount,0)) gr_amt,sum(nvl(a.IGST,0))+sum(nvl(a.CGST,0))+sum(nvl(a.SGST,0)) tax_AMT
FROM terms.invoice_header a
WHERE IDENTIFIER=$P{P_INV}
)]]>
		</queryString>
		<field name="NET_AMT_WORD" class="java.lang.String"/>
		<field name="TAX_AMT_WORD" class="java.lang.String"/>
	</subDataset>
	<subDataset name="HSNWISEDET" uuid="a12d0b92-7959-4036-9b63-5a773b8e55d0">
		<parameter name="P_INV" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT HSN_NO,NVL(CGST_RATE,0)+NVL(SGST_RATE,0) RATE,SUM(AMOUNT) TAXABLE_VAL ,NVL(SUM(SGST_AMT),0) SGST_AMT,NVL(SUM(CGST_AMT),0) CGST_AMT,NVL(SUM(IGST_AMT),0) IGST_AMT,
NVL(SUM(SGST_AMT),0)+NVL(SUM(CGST_AMT),0)+NVL(SUM(IGST_AMT),0) TAX_AMT FROM terms.INVOICE_DETAIL
WHERE INV_HEAD_IDENTIFIER=$P{P_INV}
GROUP BY HSN_NO,CGST_RATE,SGST_RATE,IGST_RATE
UNION
select 'Total',null,sum(AMOUNT),NVL(SUM(SGST_AMT),0) SGST_AMT,NVL(SUM(CGST_AMT),0) CGST_AMT,NVL(SUM(IGST_AMT),0) IGST_AMT,
NVL(SUM(SGST_AMT),0)+NVL(SUM(CGST_AMT),0)+NVL(SUM(IGST_AMT),0) TAX_AMT
FROM terms.INVOICE_DETAIL
WHERE INV_HEAD_IDENTIFIER=$P{P_INV}]]>
		</queryString>
		<field name="HSN_NO" class="java.lang.String"/>
		<field name="RATE" class="java.math.BigDecimal"/>
		<field name="TAXABLE_VAL" class="java.math.BigDecimal"/>
		<field name="SGST_AMT" class="java.math.BigDecimal"/>
		<field name="CGST_AMT" class="java.math.BigDecimal"/>
		<field name="IGST_AMT" class="java.math.BigDecimal"/>
		<field name="TAX_AMT" class="java.math.BigDecimal"/>
	</subDataset>
	<subDataset name="New Dataset 1" uuid="e6936965-316c-4d43-8575-89122cbefe7f">
		<parameter name="p_inv" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT 'Total Amount (In Words ) '||initcap(terms.AMOUNT_TO_WORD(gr_amt,'INR')) Net_amt_word,'Amount (In Words ) '||initcap(terms.AMOUNT_TO_WORD(tax_AMT,'INR')) Tax_amt_word FROM (
select sum(nvl(net_amount,0)) gr_amt,sum(nvl(a.IGST,0))+sum(nvl(a.CGST,0))+sum(nvl(a.SGST,0)) tax_AMT
FROM terms.invoice_header a
WHERE IDENTIFIER=$P{p_inv}
)]]>
		</queryString>
		<field name="NET_AMT_WORD" class="java.lang.String"/>
		<field name="TAX_AMT_WORD" class="java.lang.String"/>
	</subDataset>
	<parameter name="p_unit" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="p_inv_no" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="p_tp" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
    ROUND(ID.FR_EXC,1),
    IH.CUST_CITY_CODE,
    IH.SHIP_MODE,
    IH.PRINTED,
    IH.IDENTIFIER INV_NO,
    IH.DISTINATION,
    IH.VEHICLE_NO,
    IH.DATES,
    NVL(IH.TCS_AMT,0)TCS_AMT ,
    IH.TCS_PER,
    IH.PACKING_AMOUNT,
    TRANSPORTER_CODE,
    VOU_NO,
    VOU_DATE,
    BATCH_NUMBER,
    IH.AMOUNT_RECEIVED,
    IH.INSURANCE_VALUE,
    IH.FREIGHT_AMOUNT FREIGHT,
    IH.PACKING_AMOUNT PACKING_FORWORDING,
    IH.DISP_ADV_NO,
    IH.GR_NUMBER,
    to_char(IH.GR_DATE,'dd-MON-yy') GR_DATE,
    IH.CONSIGNEE_NAME,
    (select ADDRESS||' '||pin from terms. customer_master a where code=ih.CONSIGNEE_CODE) CONSIGNEE_ADD,
    nvl(ID.QUANTITY,0)QUANTITY,
    nvl(ID.PRICE,0)*NVL(IH.CURR_RATE,0) PRICE,
    round(ID.AMOUNT*NVL(IH.CURR_RATE,0)) TAX_AMOUNT,
    round(ID.QUANTITY*ID.PRICE*NVL(IH.CURR_RATE,0)) AMOUNT
,ID.DISCOUNT,ID.UOM,nvl(ID.IGST_AMT,0)igst_amT,ID.IGST_RATE,ID.CGST_AMT,ID.CGST_RATE,ID.SGST_AMT,ID.SGST_RATE,ID.PACK_QTY,round(ID.CESS) C_CESS,(SELECT SUBSTR(HSN_NO,1,4) from terms. ITEM_STOCK AA WHERE AA.ITEM_CD=ID.PROD_CODE) HSN_NO,
round((nvl(amount,0)-nvl(DISCOUNT,0))) tx_amt,NET_AMOUNT,VEHICLE_NO
,ID.ACK_NO PO_NO,V.NAME||' '||'('||V.CODE||')' CNAME,V.GST_REG_NO C_GSTIN,V.PAN_NO,round(amount) amount,DISCOUNT C_PAN_NO,ADDRESS||' '||pin ADDRESS,st_code,TELEPHONE,CUST_PO_NO,PAYMNT_DUE_DT,PAYMENT_TERMS,ACK_NO,
(select distinct to_char(CUST_PO_DATE,'dd-MON-yy') from terms. purchase_order_header b where b.ACK_NUMBER=ACK_NO  and AMEND_ACK_NUMBER=PO_HEAD_AMEND_ACK_NUMBER) ack_dt,
(select distinct PAYMENT_PERIOD from terms. purchase_order_header b where rownum=1 and b.ACK_NUMBER=ACK_NO) CREDIT_PD,
(SELECT ITEM_DESC from TERMS.ITEM_STOCK WHERE ITEM_CD=PROD_CODE) PROD_DESC,(SELECT ITEM_CD from terms.ITEM_STOCK WHERE ITEM_CD=PROD_CODE) ITEM_CD,
(select DISTINCT to_char(DA_DATE,'dd-MON-yy') from terms. DESPATCH_ADVICE_HEADER b where b.DA_NO=DISP_ADVICE_NO) DESP_DT,DEL_DESTINATION,CURR_NAME,CONTAINER_NO,
(select name from terms. vendor_master a where a.vendor_CODE=TRANSPORTER_CODE) trans_name,INSUR_NO,VAL_T_DATE,
INS_COMP_NAME,LC_NO,LC_date,
(select DISTINCT 'DGFT NO- '||DGFT_LIC_NO||' DATE- '||to_char(DGFT_LIC_DATE,'dd-MON-yy')||' ADV LIC NO- '||ADV_LIC_NO||' DATE- '||to_char(ADV_LIC_DATE,'dd-MON-yy')
from terms. DESPATCH_ADVICE_HEADER b where b.DA_NO=DISP_ADVICE_NO) ADGF,IH.remarks,(select DISTINCT DGFT_LIC_NO
from terms. DESPATCH_ADVICE_HEADER b where b.DA_NO=DISP_ADVICE_NO) DGFT_LIC_NO,(SELECT NAME from terms. UNIT WHERE CODE=IH.UNIT_CODE)UNIT_NAME,
(SELECT ADDRESS from terms. UNIT WHERE CODE=IH.UNIT_CODE)ADDRESS,(SELECT CIN from terms. UNIT WHERE CODE=IH.UNIT_CODE)CIN,(SELECT GST_REG_NO from terms. UNIT WHERE CODE=IH.UNIT_CODE)GST_REGUNIT,
(select DISTINCT BANK_NAME from terms. DESPATCH_ADVICE_HEADER b,terms.bank_master where BANK_CD=bank_code and b.DA_NO=DISP_ADVICE_NO) bk_name1,
(select DISTINCT BANK_NAME from terms. DESPATCH_ADVICE_HEADER b,terms.bank_master where BANK_CD1=bank_code and b.DA_NO=DISP_ADVICE_NO) bk_name2,
(select DISTINCT ADDRESS1 from terms. DESPATCH_ADVICE_HEADER b,terms.bank_master where BANK_CD=bank_code and b.DA_NO=DISP_ADVICE_NO) address1,
(select DISTINCT ADDRESS1 from terms. DESPATCH_ADVICE_HEADER b,terms.bank_master where BANK_CD1=bank_code and b.DA_NO=DISP_ADVICE_NO) address2,
(select DISTINCT ACCOUNT_NO from terms. DESPATCH_ADVICE_HEADER b,terms.bank_master where BANK_CD=bank_code and b.DA_NO=DISP_ADVICE_NO) ACCOUNT_NO1,
