		<queryString>
			<![CDATA[SELECT
 AMOUNT
,SHORT_DESC DESCRIPTION
FROM terms.emp_sal_master ESM,terms.sal_hd_mast SHM
WHERE ESM.EMPERNDED_EMP_NO=$P{p_emp_no}
AND ESM.UNIT_CD=$P{p_unit}
AND SHM.CODE=ESM.EMPERNDED_HEAD_CODE
AND ESM.EMPERNDED_HEAD_TYPE='D'
AND ESM.UNIT_CD=SHM.UNIT_CD
AND TO_CHAR(PAYMENT_FOR_MONTH,'MON')=$P{p_month}
AND TO_CHAR(PAYMENT_FOR_MONTH,'YYYY')=$P{p_year}]]>
		</queryString>
