,(select TOTAL_PAID_DAYS from terms.adv_att
WHERE emp_no=ESM.EMPERNDED_EMP_NO
and MON=$P{p_month}
AND YEAR=$P{p_year}
AND UNIT_CD=$P{p_unit})total_payble_days

,(select nvl(sum(AMOUNT),0) from TERMS.ARREAR_DETAIL
WHERE EMP_NO=ESM.EMPERNDED_EMP_NO
AND SALHD_MAST_TYPE='E'
and SAL_MONTH=$P{p_month}
AND SAL_YEAR=$P{p_year}
AND UNIT_CODE=$P{p_unit})Earning_arrear

,(select nvl(sum(AMOUNT),0) from TERMS.ARREAR_DETAIL
WHERE EMP_NO=ESM.EMPERNDED_EMP_NO
AND SAL_HD_CODE='007'
and SAL_MONTH=$P{p_month}
AND SAL_YEAR=$P{p_year}
AND UNIT_CODE=$P{p_unit})Deduction_arrear

from terms. emp_sal_master ESM
WHERE ESM.UNIT_CD=$P{p_unit}
AND TO_CHAR(ESM.PAYMENT_FOR_MONTH,'MON')=$P{p_month}
AND TO_CHAR(ESM.PAYMENT_FOR_MONTH,'YYYY')=$P{p_year}
AND ESM.EMPERNDED_EMP_NO LIKE NVL($P{p_emp_cd},'%')
AND EMP_LVL LIKE NVL($P{p_level},'%')
ORDER BY EMP_Name]]>
	</queryString>
