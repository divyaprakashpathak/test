	<queryString>
		<![CDATA[SELECT DISTINCT
ESM.EMPERNDED_EMP_NO
,ESM.PF_SALARY
,ESM.ESI_SALARY
,ESM.EMPERNDED_HEAD_TYPE
,ESM.EMPERNDED_HEAD_CODE
,ESM.AMOUNT
,ESM.EMP_DESIG_CD
,ESM.EMP_CATG
,ESM.UNIT_CD
,(SELECT NAME from terms. DEPARTMENT WHERE CODE=ESM.EMP_DEPT_CD AND UNIT_CODE=ESM.UNIT_CD )DEPARTMENT
,NVL((SELECT SUM(NVL(AMOUNT,0)) from terms. emp_sal_master WHERE EMPERNDED_EMP_NO=ESM.EMPERNDED_EMP_NO AND UNIT_CD =ESM.UNIT_CD AND EMPERNDED_HEAD_CODE='001'),0) BASICS
,ROUND(NVL((SELECT sum(NVL(AMOUNT,0)) from terms. emp_sal_master WHERE EMPERNDED_EMP_NO=ESM.EMPERNDED_EMP_NO AND UNIT_CD =ESM.UNIT_CD AND EMPERNDED_HEAD_TYPE='E' AND TO_CHAR(PAYMENT_FOR_MONTH,'MON')=$P{p_month}
AND TO_CHAR(PAYMENT_FOR_MONTH,'YYYY')=$P{p_year} GROUP BY EMPERNDED_EMP_NO ),0)) TOTAL_EAR
,ROUND(NVL((SELECT sum(NVL(AMOUNT,0)) from terms. emp_sal_master WHERE EMPERNDED_EMP_NO=ESM.EMPERNDED_EMP_NO AND UNIT_CD =ESM.UNIT_CD AND EMPERNDED_HEAD_TYPE='D' AND TO_CHAR(PAYMENT_FOR_MONTH,'MON')=$P{p_month}
AND TO_CHAR(PAYMENT_FOR_MONTH,'YYYY')=$P{p_year} GROUP BY EMPERNDED_EMP_NO  ),0)) TOTAL_DED
,(ROUND((NVL((SELECT sum(NVL(AMOUNT,0)) from terms. emp_sal_master WHERE EMPERNDED_EMP_NO=ESM.EMPERNDED_EMP_NO AND UNIT_CD =ESM.UNIT_CD AND EMPERNDED_HEAD_TYPE='E' AND TO_CHAR(PAYMENT_FOR_MONTH,'MON')=$P{p_month}
AND TO_CHAR(PAYMENT_FOR_MONTH,'YYYY')=$P{p_year}  GROUP BY EMPERNDED_EMP_NO ),0)))-(ROUND(NVL((SELECT sum(NVL(AMOUNT,0)) from terms. emp_sal_master WHERE EMPERNDED_EMP_NO=ESM.EMPERNDED_EMP_NO AND UNIT_CD =ESM.UNIT_CD AND EMPERNDED_HEAD_TYPE='D' AND TO_CHAR(PAYMENT_FOR_MONTH,'MON')=$P{p_month}
AND TO_CHAR(PAYMENT_FOR_MONTH,'YYYY')=$P{p_year} GROUP BY EMPERNDED_EMP_NO ),0)))) NET
,INITCAP(TERMS.AMOUNT_TO_WORD((ROUND((NVL((SELECT sum(NVL(AMOUNT,0)) from terms. emp_sal_master WHERE EMPERNDED_EMP_NO=ESM.EMPERNDED_EMP_NO AND UNIT_CD =ESM.UNIT_CD AND EMPERNDED_HEAD_TYPE='E' AND TO_CHAR(PAYMENT_FOR_MONTH,'MON')=$P{p_month}
AND TO_CHAR(PAYMENT_FOR_MONTH,'YYYY')=$P{p_year}  GROUP BY EMPERNDED_EMP_NO ),0)))-(ROUND(NVL((SELECT sum(NVL(AMOUNT,0)) from terms. emp_sal_master WHERE EMPERNDED_EMP_NO=ESM.EMPERNDED_EMP_NO AND UNIT_CD =ESM.UNIT_CD AND EMPERNDED_HEAD_TYPE='D' AND TO_CHAR(PAYMENT_FOR_MONTH,'MON')=$P{p_month}
AND TO_CHAR(PAYMENT_FOR_MONTH,'YYYY')=$P{p_year} GROUP BY EMPERNDED_EMP_NO ),0)))),'INR')) NET_IN_WORD
,(SELECT NAME from terms. UNIT WHERE CODE=ESM.UNIT_CD) U_NAME
,(SELECT ADDRESS from terms. UNIT WHERE CODE=ESM.UNIT_CD) U_ADDRESS
,(SELECT EMP_FIRST_NAME||' '||EMP_LAST_NAME from terms. EMP_MASTER WHERE EMP_NUMBER=ESM.EMPERNDED_EMP_NO) EMP_NAME
,(SELECT DESCRIPTION from terms. sal_hd_mast WHERE CODE=ESM.EMPERNDED_HEAD_CODE AND UNIT_CD=ESM.UNIT_CD ) ERN_DEDCN_DESC
,(SELECT DESCRIPTION from terms. DESIGNATION WHERE CODE=ESM.EMP_DESIG_CD AND UNIT_CD=ESM.UNIT_CD ) DESINATION_DESC
,(SELECT PF_NUMBER from terms. EMP_MASTER WHERE EMP_NUMBER=ESM.EMPERNDED_EMP_NO AND UNIT_CD=ESM.UNIT_CD) PF_CODE
,(SELECT ESI_NUMBER from terms. EMP_MASTER WHERE EMP_NUMBER=ESM.EMPERNDED_EMP_NO AND UNIT_CD=ESM.UNIT_CD)ESI_NUMBER
,(SELECT BANK_ACC_NUMBER from terms. EMP_MASTER WHERE EMP_NUMBER=ESM.EMPERNDED_EMP_NO AND UNIT_CD=ESM.UNIT_CD)BANK_ACC_NUMBER
,(SELECT DISTINCT UAN_NO from terms. EMP_MASTER WHERE EMP_NUMBER=ESM.EMPERNDED_EMP_NO AND UNIT_CD=ESM.UNIT_CD )UAN_NO
,(SELECT PAN_NO FROM terms.Emp_master_hd emh WHERE EMP_NUMBER=ESM.EMPERNDED_EMP_NO AND UNIT=ESM.UNIT_CD)PAN_NO
,(SELECT ADHAAR_NO FROM terms.Emp_master_hd emh WHERE EMP_NUMBER=ESM.EMPERNDED_EMP_NO AND UNIT=ESM.UNIT_CD)ADHAAR_NO
,(SELECT JOIN_DATE FROM terms.Emp_master_hd emh WHERE EMP_NUMBER=ESM.EMPERNDED_EMP_NO AND UNIT=ESM.UNIT_CD)JOIN_DATE
,(SELECT FATHER_NAME FROM terms.Emp_master_hd emh WHERE EMP_NUMBER=ESM.EMPERNDED_EMP_NO AND UNIT=ESM.UNIT_CD)FATHER_NAME
,(SELECT WORK_DAYS from terms. WORKING_DAYS WHERE UNIT_CD=ESM.UNIT_CD AND MONTH=$P{p_month} AND YEAR=$P{p_year}) NO_WORKING
,(SELECT NO_OF_DAYS from terms. ATTEND_MASTER WHERE UNIT_CD=ESM.UNIT_CD AND MONTH=$P{p_month} AND EMP_NO=ESM.EMPERNDED_EMP_NO AND ATTEND_LEAVE_TYPE='AB'AND YEAR=$P{p_year}) ABS_DAYS
,TERMS.EARNINGS (ESM.EMPERNDED_EMP_NO,ESM.UNIT_CD,ESM.EMPERNDED_HEAD_CODE) EAR
,TERMS.DEDUCTIONS (ESM.EMPERNDED_EMP_NO,ESM.UNIT_CD,ESM.EMPERNDED_HEAD_CODE) DED
,to_char(ESM.PAYMENT_FOR_MONTH,'MON-YYYY')as payment_for_mon


