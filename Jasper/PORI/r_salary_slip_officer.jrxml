	<subDataset name="EMP_ATTEND" uuid="37f947f0-f670-49d4-b101-68df02361195">
		<parameter name="p_unit" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="p_mon" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="p_year" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="p_emp" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[with data as(SELECT ATTEND_LEAVE_TYPE,NO_OF_DAYS FROM terms.ATTEND_MASTER WHERE UNIT_CODE=$P{p_unit}
AND MONTH=$P{p_mon}
AND YEAR=$P{p_year}
AND EMP_NO LIKE NVL($P{p_emp},'%'))
select * from data
pivot(
MAX(ATTEND_LEAVE_TYPE||': '||case when NO_OF_DAYS||'    '>0 and NO_OF_DAYS||'    '<1 then '0'||to_char(NO_OF_DAYS||'    ')
else to_char(NO_OF_DAYS||'    ')
end) for ATTEND_LEAVE_TYPE in ('FO','EL','CL','OD','CO','HO','SL','AB','LW','PR','WO')
)]]>
		</queryString>
		<field name="&apos;FO&apos;" class="java.lang.String"/>
		<field name="&apos;EL&apos;" class="java.lang.String"/>
		<field name="&apos;CL&apos;" class="java.lang.String"/>
		<field name="&apos;OD&apos;" class="java.lang.String"/>
