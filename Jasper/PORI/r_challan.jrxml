	<subDataset name="num_to_word" uuid="f245341f-30f4-42c0-bcbc-eed8469d75f3">
		<parameter name="p_no" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT TERMS.AMOUNT_TO_WORD(ROUND (AMOUNT1 , 2) ,'INR') NUM_WRD
FROM(
select SUM(
            CASE when C.CGST_AMT is null then nvl(C.AMT*(C.CGST/100),0) else 0 end +
    CASE when C.IGST_AMT is null then nvl(C.AMT*(C.IGST/100),0) else 0 end +
    CASE when C.SGST_AMT is null then nvl(C.AMT*(C.SGST/100),0) else 0 end+
    NVL(C.AMT,0)
          )AMOUNT1
FROM TERMS.CCGP_DETLS c
WHERE c.CCGP_HEAD_CHALLAN_NO=$P{p_no})]]>
		</queryString>
		<field name="NUM_WRD" class="java.lang.String"/>
	</subDataset>
