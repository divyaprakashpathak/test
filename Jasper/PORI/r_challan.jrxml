	</parameter>
	<parameter name="p_tp" class="java.lang.String"/>
	<queryString>
		<![CDATA[WITH UNIT_TEMP AS (select LOGO_PATH from terms.unit where rownum = 1 order by code)
SELECT
NAME,SBU_CODE,SHIP_TO,CODE,ADDRESS,CHALLAN_NO,CHALLAN_DT,CHALLAN_TYPE,CHALLAN_DESC,

DECODE (CHALLAN_DESC, 'Line Rejection','',
 'General FOC','Certified that the above goods are being sent for FOC Purpose only and not meant for Sale and has no Commercial Value',
 'General Non-Foc (Payable)','Certified that the above goods are being sent for FOC Purpose only and not meant for Sale and has no Commercial Value',
 'SRV Rejection','','Sales Invoice','',
 'Returnable Gate Pass (RGP)','Certified that the above goods are being sent for Job Work/Repair/Returned and not meant for Sale and has no Commercial Value') CHALLAN_DESC_REMARK,

 PARTY_CD, PARTY_NAME, EXIT_TYPE, OC_AMOUNT, DEL_MODE, VEHICLE_NO, EWAY_NO, EWAY_DATE, GATE_EXIT_DT, REMARK, RET_RESP_EMP, TARGET_DATE, EMP_CD, EMP_FIRST_NAME,
 EMP_LAST_NAME, DESCRIPT, ITEM_DESC, UOM, QTY, ITEM_CD, AMT, CGST, IGST,SGST,CGST_AMT,IGST_AMT,SGST_AMT,RATE,HSN_NO,TOTAL_AMOUNT,PAN_NO,GST_REG_NO,DRIVER_NAME,
 DRIVER_CONTACT,AUTH_BY,SBU_FLG,Exit_type1,STATUS,status_Desc,Cons_Phones,Cons_Pin,Cons_city,Cons_state,party_Phones,party_Pin,party_city,party_state,CONSIGNEE,
 CONSIGNEE_ADDRESS,CONSIGNEE_PANNO,CONSIGNEE_GST_NO,PARTY_ADDRESS,REPRESENTATIVE,LOGO_PATH

FROM
(SELECT
A.NAME,
A.SBU_CODE,
B.SHIP_TO,
A.CODE,
A.ADDRESS,
B.CHALLAN_NO,
B.CHALLAN_DT,
B.CHALLAN_TYPE,
(SELECT SEV.DESCRIPTION
 FROM TERMS.SEC_CONTROL_VALUES SEV
 WHERE B.CHALLAN_TYPE = SEV.CONTROL_CODE
 AND SEV.CONTROL_TYPE = 'CHALLAN_TYPE') AS CHALLAN_DESC,
DECODE(B.CHALLAN_TYPE,'AS','OTHER','JS','JOB WORK','AG','OTHER','FO','OTHERS','SR','OTHER ','LR','OTHER ')CHALLAN_TYPE12,
B.PARTY_CD,
B.PARTY_NAME,
B.EXIT_TYPE,
B.OC_AMOUNT,
B.DEL_MODE,
B.VEHICLE_NO,
B.EWAY_NO,
B.EWAY_DATE,
B.GATE_EXIT_DT,
B.FOR_PROCES REMARK,
B.RET_RESP_EMP,
B.PGT_DATE TARGET_DATE,
B.EMP_CD,
EMP_FIRST_NAME,
EMP_LAST_NAME,
C.DESCRIPT,
D.ITEM_DESC,
D.ISS_UOM UOM,
C.QTY,
C.ITEM_CD,
C.AMT,
C.CGST,
C.IGST,
C.SGST,
CASE when C.CGST_AMT is null then C.AMT*(C.CGST/100) end CGST_AMT,
CASE when C.SGST_AMT is null then C.AMT*(C.SGST/100) end SGST_AMT,
CASE when C.IGST_AMT is null then C.AMT*(C.IGST/100) end IGST_AMT,
C.AMT/C.QTY RATE,
SUBSTR(D.HSN_NO,1,6)HSN_NO,
(CASE when C.CGST_AMT is null then nvl(C.AMT*(C.CGST/100),0) else 0 end +
    CASE when C.IGST_AMT is null then nvl(C.AMT*(C.IGST/100),0) else 0 end +
    CASE when C.SGST_AMT is null then nvl(C.AMT*(C.SGST/100),0) else 0 end+
    NVL(C.AMT,0)) TOTAL_AMOUNT,
F.PAN_NO,
F.GST_REG_NO,
B.DRIVER_NAME,
B.DRIVER_CONTACT,
AUTH_BY,
decode(A.SBU_CODE, 101, 'Y', 'N') SBU_FLG,
decode(B.EXIT_TYPE, 'N','Non-Returnable','Returnable')Exit_type1,
B.STATUS,
decode(B.STATUS,'C','CANCELLED')status_Desc,


(Select PHONES From Terms.VENDOR_REGD_ADDRESS WHERE  VENDOR_CODE = B.SHIP_TO)Cons_Phones,
(Select PIN_CODE From Terms.VENDOR_REGD_ADDRESS WHERE  VENDOR_CODE = B.SHIP_TO)Cons_Pin,
(Select CITY From Terms.VENDOR_REGD_ADDRESS WHERE  VENDOR_CODE = B.SHIP_TO)Cons_city,
(Select STATE From Terms.VENDOR_REGD_ADDRESS WHERE  VENDOR_CODE = B.SHIP_TO)Cons_state,
(Select PHONES From Terms.VENDOR_REGD_ADDRESS WHERE  VENDOR_CODE = B.PARTY_CD)party_Phones,
(Select PIN_CODE From Terms.VENDOR_REGD_ADDRESS WHERE  VENDOR_CODE = B.PARTY_CD)party_Pin,
(Select CITY From Terms.VENDOR_REGD_ADDRESS WHERE  VENDOR_CODE = B.PARTY_CD)party_city,
(Select STATE From Terms.VENDOR_REGD_ADDRESS WHERE  VENDOR_CODE = B.PARTY_CD)party_state,
(SELECT NAME FROM  Terms.VENDOR_MASTER h WHERE  h.VENDOR_CODE = B.SHIP_TO
union
SELECT NAME FROM  Terms.MANUFACT_MASTER h WHERE  h.CODE = B.SHIP_TO) CONSIGNEE,
(SELECT ADDRESS1||''||ADDRESS2||''||ADDRESS3 FROM  Terms.VENDOR_REGD_ADDRESS WHERE  VENDOR_CODE = B.SHIP_TO
union
SELECT ADDRESS1||''||ADDRESS2 FROM  Terms.MANUFACT_MASTER h WHERE  h.CODE = B.SHIP_TO
) CONSIGNEE_ADDRESS,
(SELECT PAN_NO FROM  Terms.VENDOR_MASTER  WHERE  VENDOR_CODE = B.SHIP_TO
union
SELECT TIN_NO FROM Terms. MANUFACT_MASTER h WHERE  h.CODE = B.SHIP_TO)CONSIGNEE_PANNO,
(SELECT GST_REG_NO FROM  Terms.VENDOR_MASTER WHERE  VENDOR_CODE = B.SHIP_TO
union
SELECT ECC_NO FROM  Terms.MANUFACT_MASTER h WHERE  h.CODE = B.SHIP_TO)CONSIGNEE_GST_NO,
(SELECT ADDRESS1||' '||ADDRESS2||''||ADDRESS3 FROM  Terms.VENDOR_REGD_ADDRESS WHERE  VENDOR_CODE = B.PARTY_CD ) PARTY_ADDRESS,
(SELECT EMP_FIRST_NAME||' '||EMP_LAST_NAME FROM  Terms.EMP_MASTER WHERE  EMP_NUMBER = B.RET_RESP_EMP ) REPRESENTATIVE,
(SELECT LOGO_PATH FROM TERMS.UNIT_TEMP) LOGO_PATH
FROM Terms.UNIT A,Terms.CCGP_DETLS C,Terms.ITEM_STOCK D,Terms.VENDOR_REGD_ADDRESS E,Terms.VENDOR_MASTER F,Terms.CCGP_HEAD B
LEFT JOIN TERMS.EMP_MASTER_HD ON EMP_CD = EMP_NUMBER



WHERE
A.CODE=$P{p_unit}
AND A.CODE=B.UNIT_CD
AND B.CHALLAN_NO=$P{p_no}
AND B.CHALLAN_NO=C.CCGP_HEAD_CHALLAN_NO
AND C.ITEM_CD=D.ITEM_CD
AND E.VENDOR_CODE=B.PARTY_CD
AND B.PARTY_CD=F.VENDOR_CODE)A]]>
	</queryString>
