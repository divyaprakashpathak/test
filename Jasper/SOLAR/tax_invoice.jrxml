	<subDataset name="AMT_WORD" uuid="206fe128-4870-4527-b3d2-d3d12fdecfba">
		<parameter name="P_INV" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT 'Grand Total (In Words ) '||initcap(TERMS.AMOUNT_TO_WORD(gr_amt,'INR')) Net_amt_word,'GST Amount (In Words ) '||initcap(TERMS.AMOUNT_TO_WORD(tax_AMT,'INR')) Tax_amt_word FROM (
select  sum(NVL(NET_AMOUNT,0))  gr_amt,round(sum(nvl(a.IGST,0))+sum(nvl(a.CGST,0))+sum(nvl(a.SGST,0)),2) tax_AMT
FROM TERMS.invoice_header a
WHERE IDENTIFIER=$P{P_INV}
)]]>
		</queryString>
		<field name="NET_AMT_WORD" class="java.lang.String"/>
		<field name="TAX_AMT_WORD" class="java.lang.String"/>
	</subDataset>
	<subDataset name="HSNWISEDET" uuid="85c5ae74-9524-42c6-8be0-337cea62ab24">
		<parameter name="P_INV" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT HSN_NO,
        NVL(CGST_RATE,0)+NVL(SGST_RATE,0)+NVL(IGST_RATE,0) RATE,
        TO_CHAR(SUM(AMOUNT),'99G99G99G99G990D99') TAXABLE_VAL,
        TO_CHAR(NVL(SUM(SGST_AMT),0),'99G99G99G99G990D99') SGST_AMT,
        TO_CHAR(NVL(SUM(CGST_AMT),0),'99G99G99G99G990D99') CGST_AMT,
        TO_CHAR(NVL(SUM(IGST_AMT),0),'99G99G99G99G990D99') IGST_AMT,
        TO_CHAR(NVL(SUM(SGST_AMT),0)+NVL(SUM(CGST_AMT),0)+NVL(SUM(IGST_AMT),0),'99G99G99G99G990D99') TAX_AMT ,
        nvl(sum(QUANTITY),0)REQ_HSN_QTY
FROM TERMS.INVOICE_DETAIL
WHERE INV_HEAD_IDENTIFIER=$P{P_INV}
GROUP BY HSN_NO,CGST_RATE,SGST_RATE,IGST_RATE

UNION
select 'Total',null,
        TO_CHAR(sum(AMOUNT),'99G99G99G99G990D99'),
        TO_CHAR(NVL(SUM(SGST_AMT),0),'99G99G99G99G990D99') SGST_AMT,
        TO_CHAR(NVL(SUM(CGST_AMT),0),'99G99G99G99G990D99') CGST_AMT,
        TO_CHAR(NVL(SUM(IGST_AMT),0),'99G99G99G99G990D99') IGST_AMT,
        TO_CHAR(NVL(SUM(SGST_AMT),0)+NVL(SUM(CGST_AMT),0)+NVL(SUM(IGST_AMT),0),'99G99G99G99G990D99') TAX_AMT,
        nvl(sum(QUANTITY),0) REQ_HSN_QTY
 FROM TERMS.INVOICE_DETAIL
WHERE INV_HEAD_IDENTIFIER=$P{P_INV}]]>
		</queryString>
		<field name="HSN_NO" class="java.lang.String"/>
		<field name="RATE" class="java.math.BigDecimal"/>
		<field name="TAXABLE_VAL" class="java.lang.String"/>
		<field name="SGST_AMT" class="java.lang.String"/>
		<field name="CGST_AMT" class="java.lang.String"/>
		<field name="IGST_AMT" class="java.lang.String"/>
		<field name="TAX_AMT" class="java.lang.String"/>
		<field name="REQ_HSN_QTY" class="java.math.BigDecimal"/>
	</subDataset>
	<subDataset name="QR_SET_1" uuid="d5aea40d-68a5-4e01-8dc0-4b891cd85161">
		<parameter name="p_inv_no" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[WITH DEATILS AS
(

Select ID1.INV_HEAD_IDENTIFIER,
    MIN(PROD_Code) PROD_Code
    ,MIN(ID1.IGST_AMT) IGST_AMT
    ,MIN(ID1.CGST_AMT) CGST_AMT
    ,MIN(ID1.SGST_AMT) SGST_AMT
  From  TERMS.INVOICE_DETAIL ID1
Where (ID1.INV_HEAD_IDENTIFIER, ID1.PRICE)in
    (SELECT ID.INV_HEAD_IDENTIFIER, MAX(ID.PRICE)
FROM  TERMS.INVOICE_DETAIL ID
     GROUP BY ID.INV_HEAD_IDENTIFIER)
    group by ID1.INV_HEAD_IDENTIFIER)


SELECT
    IH.IDENTIFIER,
    IH.DATES,
    DS.PROD_Code,
(SELECT ITEM_DESC FROM TERMS.ITEM_STOCK WHERE ITEM_CD=DS.PROD_CODE
       UNION
   SELECT ITEM_DESC FROM TERMS.INV_SER_BILL_VIEW WHERE ITEM_CD=DS.PROD_CODE) PROD_DESC,
    UT.GST_REG_NO
    ,UT.NAME
    ,v.REG_FLAG
    ,DS.IGST_AMT
    ,DS.CGST_AMT
    ,DS.SGST_AMT
   , 0 CESS
    ,NET_AMOUNT
    ,UT.PAYMENT_UPI_ID
    ,UT.PAYEE_ACCOUNT_NO
    ,UT.PAYEE_ACCOUNT_IFSC

FROM
    TERMS. INVOICE_HEADER IH,
    TERMS.vendor_master V,
    terms.UNIT UT,
    DEATILS DS
WHERE
     IH.IDENTIFIER = $P{p_inv_no}
 	AND DS.INV_HEAD_IDENTIFIER = IH.IDENTIFIER
     AND V.VENDOR_CODE = IH.CUST_CODE
     AND UT.CODE = IH.UNIT_CODE]]>
		</queryString>
		<field name="IDENTIFIER" class="java.lang.String"/>
		<field name="DATES" class="java.sql.Timestamp"/>
		<field name="PROD_CODE" class="java.lang.String"/>
		<field name="PROD_DESC" class="java.lang.String"/>
		<field name="GST_REG_NO" class="java.lang.String"/>
		<field name="NAME" class="java.lang.String"/>
		<field name="REG_FLAG" class="java.lang.String"/>
		<field name="IGST_AMT" class="java.math.BigDecimal"/>
		<field name="CGST_AMT" class="java.math.BigDecimal"/>
		<field name="SGST_AMT" class="java.math.BigDecimal"/>
		<field name="CESS" class="java.math.BigDecimal"/>
		<field name="NET_AMOUNT" class="java.math.BigDecimal"/>
		<field name="PAYMENT_UPI_ID" class="java.lang.String"/>
		<field name="PAYEE_ACCOUNT_NO" class="java.lang.String"/>
		<field name="PAYEE_ACCOUNT_IFSC" class="java.lang.String"/>
	</subDataset>
	<parameter name="p_unit" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="p_inv_no" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[WITH DATA AS (
SELECT 1 GRP,
IH.STOCK_TYPE,
IH.CANCELLED,
ID.PACK_QTY,
ID.PACK_UOM,
IH.REMARKS,
IH.IRN_NO,
IH.LOCATION,
IH.QR_INFO,
IH.APPROVE_CODE,
IH.CUST_CITY_CODE,
IH.SHIP_MODE,
IH.IDENTIFIER INV_NO,
IH.DISTINATION,
IH.VEHICLE_NO,
IH.DATES,
TO_CHAR(NVL(IH.TCS_AMT,0),'99G99G99G99G990D99')TCS_AMT ,
IH.TCS_PER,
IH.PACKING_AMOUNT,
TRANSPORTER_CODE,
VOU_NO,
ID.PROD_CODE,
VOU_DATE,
IH.AMOUNT_RECEIVED,
IH.INSURANCE_VALUE,
IH.FREIGHT_AMOUNT FREIGHT,
IH.PACKING_AMOUNT PACKING_FORWORDING,
IH.DISP_ADV_NO,
IH.GR_NUMBER,
TO_CHAR(IH.GR_DATE,'dd-MON-yy') GR_DATE,
IH.CONSIGNEE_NAME,
(SELECT ADDRESS1||' '||ADDRESS2 || ' ' || PIN_CODE FROM TERMS. VENDOR_REGD_ADDRESS A WHERE A.VENDOR_CODE = IH.CONSIGNEE_CODE) CONSIGNEE_ADD,
ID.QUANTITY QUANTITY ,
ID.PRICE*NVL(IH.CURR_RATE,0) PRICE,
ROUND(ID.AMOUNT*NVL(IH.CURR_RATE,0))  TAX_AMOUNT,
ROUND(ID.QUANTITY*ID.PRICE*NVL(IH.CURR_RATE,0)) AMOUNT,
ID.DISCOUNT,
ID.UOM,
NVL(ID.IGST_AMT,0) IGST_AMT,
ID.IGST_RATE,
NVL(ID.CGST_AMT,0) CGST_AMT,
ID.CGST_RATE,
NVL(ID.SGST_AMT,0)SGST_AMT,
ID.SGST_RATE,
TO_CHAR(SUM(NVL(ID.IGST_AMT,0)) OVER (PARTITION BY IH.IDENTIFIER),'99G99G99G99G990D99')SUM_IGST,
TO_CHAR(SUM(NVL(ID.CGST_AMT,0)) OVER (PARTITION BY IH.IDENTIFIER),'99G99G99G99G990D99')SUM_CGST,
TO_CHAR(SUM(NVL(ID.SGST_AMT,0)) OVER (PARTITION BY IH.IDENTIFIER),'99G99G99G99G990D99')SUM_SGST,
ROUND(ID.CESS) C_CESS,
ID.HSN_NO HSN_NO,
NVL(ID.AMOUNT,0) TX_AMT,
TO_CHAR(SUM(NVL(ID.AMOUNT,0))OVER(PARTITION BY IH.IDENTIFIER),'99G99G99G99G990D99') SUM_TX_AMT,
TO_CHAR((NET_AMOUNT-IH.TCS_AMT),'99G99G99G99G990D99') NET_AMOUNT,
TO_CHAR(NET_AMOUNT,'99G99G99G99G990D99')NET_AMT_TCS_AMT,
--TO_CHAR((NET_AMOUNT+NVL(ID.IGST_AMT,0) +NVL(ID.CGST_AMT,0)+NVL(ID.SGST_AMT,0) +ROUND(NVL(IH.CESS_AMT,0))),'99G99G99G99G990D99') SUM_TOT_NET_AMT,
ID.ACK_NO PO_NO,
V.NAME||' '||'('||V.VENDOR_CODE||')' CNAME,
V.GST_REG_NO C_GSTIN,
V.PAN_NO,
V.LEGAL_NAME,
SUM(ID.AMOUNT)OVER(PARTITION BY ID.PROD_CODE) HEAD_AMOUNT,
DISCOUNT C_PAN_NO,
VA.ADDRESS1||' '||VA.ADDRESS2||' '||VA.PIN_CODE ADDRESS,
ST_CODE,
VA.PHONES TELEPHONE,
CUST_PO_NO,
ID.CUST_PO_DT,
PAYMNT_DUE_DT,
PAYMENT_TERMS,
ACK_NO,
(SELECT DISTINCT TO_CHAR(CUST_PO_DATE,'dd-MM-RRRR')
       FROM TERMS. PURCHASE_ORDER_HEADER B
       WHERE B.ACK_NUMBER=ACK_NO
       AND AMEND_ACK_NUMBER=PO_HEAD_AMEND_ACK_NUMBER) ACK_DT,
(SELECT DISTINCT PAYMENT_PERIOD
       FROM TERMS. PURCHASE_ORDER_HEADER B
       WHERE ROWNUM=1 AND B.ACK_NUMBER=ACK_NO) CREDIT_PD,
(SELECT ITEM_DESC FROM TERMS.ITEM_STOCK WHERE ITEM_CD=PROD_CODE
       UNION
   SELECT ITEM_DESC FROM TERMS.INV_SER_BILL_VIEW WHERE ITEM_CD=PROD_CODE) PROD_DESC,
(SELECT ITEM_CD FROM TERMS. INV_SER_BILL_VIEW WHERE ITEM_CD=PROD_CODE
       UNION
   SELECT ITEM_CD FROM TERMS.ITEM_STOCK WHERE ITEM_CD=PROD_CODE) ITEM_CD,
(SELECT DISTINCT TO_CHAR(DA_DATE,'dd-MON-yy')
       FROM TERMS. DESPATCH_ADVICE_HEADER B
       WHERE B.DA_NO=DISP_ADVICE_NO) DESP_DT,
CURR_NAME,
CONTAINER_NO,
(SELECT NAME FROM TERMS. VENDOR_MASTER A WHERE A.VENDOR_CODE=TRANSPORTER_CODE) TRANS_NAME,
INSUR_NO,
VAL_T_DATE,
INS_COMP_NAME,
LC_NO,
LC_DATE,
(SELECT DISTINCT 'DGFT NO- '||DGFT_LIC_NO||' DATE- '||TO_CHAR(DGFT_LIC_DATE,'dd-MON-yy')||' ADV LIC NO- '
     ||ADV_LIC_NO||' DATE- '||TO_CHAR(ADV_LIC_DATE,'dd-MON-yy')
     FROM TERMS. DESPATCH_ADVICE_HEADER B
     WHERE B.DA_NO=DISP_ADVICE_NO) ADGF,
(SELECT DISTINCT DGFT_LIC_NO
      FROM TERMS. DESPATCH_ADVICE_HEADER B
      WHERE B.DA_NO=DISP_ADVICE_NO) DGFT_LIC_NO,
(SELECT NAME FROM TERMS. UNIT WHERE CODE=IH.UNIT_CODE)UNIT_NAME,
(SELECT ADDRESS FROM TERMS. UNIT WHERE CODE=IH.UNIT_CODE)UNIT_ADDRESS,
(SELECT EMAIL FROM TERMS.UNIT WHERE CODE=IH.UNIT_CODE)EMAIL,
(SELECT CIN FROM TERMS. UNIT WHERE CODE=IH.UNIT_CODE)CIN,(SELECT GST_REG_NO FROM TERMS. UNIT WHERE CODE=IH.UNIT_CODE)GST_REGUNIT,
(SELECT DISTINCT BANK_NAME FROM TERMS. DESPATCH_ADVICE_HEADER B,TERMS.BANK_MASTER WHERE BANK_CD=BANK_CODE AND B.DA_NO=DISP_ADVICE_NO) BK_NAME1,
(SELECT DISTINCT BANK_NAME FROM TERMS. DESPATCH_ADVICE_HEADER B,TERMS.BANK_MASTER WHERE BANK_CD1=BANK_CODE AND B.DA_NO=DISP_ADVICE_NO) BK_NAME2,
(SELECT DISTINCT ADDRESS1 FROM TERMS. DESPATCH_ADVICE_HEADER B,TERMS.BANK_MASTER WHERE BANK_CD=BANK_CODE AND B.DA_NO=DISP_ADVICE_NO) ADDRESS1,
(SELECT DISTINCT ADDRESS1 FROM TERMS. DESPATCH_ADVICE_HEADER B,TERMS.BANK_MASTER WHERE BANK_CD1=BANK_CODE AND B.DA_NO=DISP_ADVICE_NO) ADDRESS2,
(SELECT DISTINCT ACCOUNT_NO FROM TERMS. DESPATCH_ADVICE_HEADER B,TERMS.BANK_MASTER WHERE BANK_CD=BANK_CODE AND B.DA_NO=DISP_ADVICE_NO) ACCOUNT_NO1,
(SELECT DISTINCT ACCOUNT_NO FROM TERMS. DESPATCH_ADVICE_HEADER B,TERMS.BANK_MASTER WHERE BANK_CD1=BANK_CODE AND B.DA_NO=DISP_ADVICE_NO) ACCOUNT_NO2,
(SELECT DISTINCT IFSC_CODE FROM TERMS. DESPATCH_ADVICE_HEADER B,TERMS.BANK_MASTER WHERE BANK_CD=BANK_CODE AND B.DA_NO=DISP_ADVICE_NO) IFSC_CODE1,
(SELECT STATE_CODE FROM TERMS. VENDOR_REGD_ADDRESS A WHERE A.VENDOR_CODE = CONSIGNEE_CODE) CON_ST_CODE,
(SELECT DISTINCT DEL_DESTINATION FROM TERMS. CUSTOMER_MASTER A WHERE CODE=CONSIGNEE_CODE) CON_DEL_DESTINATION,
(SELECT GST_REG_NO FROM TERMS. VENDOR_MASTER A WHERE A.VENDOR_CODE = CONSIGNEE_CODE) CON_GST_REG_NO,
(SELECT PIN_CODE FROM TERMS. VENDOR_REGD_ADDRESS A WHERE A.VENDOR_CODE=CONSIGNEE_CODE) CON_PIN,
(SELECT NAME||' '||'('||VENDOR_CODE||')' FROM TERMS. VENDOR_MASTER WHERE VENDOR_CODE=IH.CONSIGNEE_CODE) CON_NAME,
(SELECT PHONES FROM TERMS. VENDOR_REGD_ADDRESS A WHERE A.VENDOR_CODE=CONSIGNEE_CODE) CON_TELEPHONE,
DECODE(INV_HEAD_TYPE,'G','DELIVERY CHALLAN','E','DELIVERY CHALLAN','M','DELIVERY CHALLAN','U','DELIVERY CHALLAN','W','DELIVERY CHALLAN','A','CREDIT NOTE',
     'H','DEBIT NOTE','Z','BILL OF SUPPLY','Y','BILL OF SUPPLY','TAX INVOICE') TP,DECODE(INV_HEAD_TYPE,'G','DELIVERY CHALLAN','E','DELIVERY CHALLAN','M',
     'DELIVERY CHALLAN','U','DELIVERY CHALLAN','W','DELIVERY CHALLAN','A','CREDIT NOTE',
     'H','DEBIT NOTE','Z','INVOICE','Y','INVOICE','TAX INVOICE') TP1,
(SELECT DISTINCT IFSC_CODE
     FROM TERMS. DESPATCH_ADVICE_HEADER B,
          TERMS.BANK_MASTER
     WHERE BANK_CD1=BANK_CODE
     AND B.DA_NO=DISP_ADVICE_NO) IFSC_CODE2,
--initcap(TERMS.amount_to_word(SUM(NVL(id.AMOUNT,0))OVER(PARTITION BY ID.PROD_CODE),'INR')) amt_to_word,
EWAY_NO,
IH.EWAY_DATE,
VA.PIN_CODE PIN,
DECODE(INV_HEAD_TYPE,'G','Transfer Note No / Date','E','Transfer Note No / Date','M','Transfer Note No / Date','U','Transfer Note No / Date','W',
    'Transfer Note No / Date','A','','H','','Sale Note No / Date') PT_TP,
DECODE(INV_HEAD_TYPE,'G','Transfer Order No. / Date','E','Transfer Note No / Date','M','Transfer Order No. / Date','U',
    'Transfer Order No. / Date','W','Transfer Order No. / Date','A','','H','',
    'Purchase Order No. / Date') PT_DT,
DECODE(INV_HEAD_TYPE,'G','Total Challan Value (In Figures)','E','Total Challan Value (In Figures)','M','Total Challan Value (In Figures)',
    'U','Total Challan Value (In Figures)','W','Total Challan Value (In Figures)','A','Total Credit Value (In Figures)',
    'H','Total Credit Value (In Figures)','Total Invoice Value (In Figures)') INV_VAL_FG,
DECODE(INV_HEAD_TYPE,'G','Total Challan Value (In Words)','E','Total Challan Value (In Words)','M','Total Challan Value (In Words)','U','Total Challan Value (In Words)','W',
       'Total Challan Value (In Words)','A','Total Credit Value (In Words)','H','Total Credit Value (In Words)',
       'Total Invoice Value (In Words)') INV_VAL_WD,
NET_AMOUNT TOTAMT,
ID.PACK_QTY||' '||'('||(SELECT DISTINCT PACK_DESC
                         FROM TERMS. PACKING_DETAIL A
                         WHERE A.PACK_CODE=(SELECT DISTINCT PACK_CODE
                                            FROM TERMS. DESPATCH_ADVICE_DETAIL
                                            WHERE DA_NO=ID.DISP_ADVICE_NO
                                            AND PRODUCT_CODE=ID.PROD_CODE
                                            AND DA_HEAD_AMD_NO=(SELECT MAX(DA_HEAD_AMD_NO)
                                                                FROM TERMS. DESPATCH_ADVICE_DETAIL
                                                                WHERE DA_NO=ID.DISP_ADVICE_NO)))||')'||' ' PACK_DD,
DECODE(INSUR_TYPE,'U','By Us','P','By Party') INS_TP,
DECODE(INV_HEAD_TYPE,'E','Y','N') TYP,
ID.CESS_PER,
ID.CESS_AMT,
ROUND(NVL(IH.CESS_AMT,0)) CESS,
INV_HEAD_TYPE,
(SELECT UOM FROM TERMS. DESPATCH_ADVICE_DETAIL
    WHERE PRODUCT_CODE=ID.PROD_CODE
    AND DA_NO=ID.DISP_ADVICE_NO
    AND ROWNUM=1
    AND DA_HEAD_AMD_NO=(SELECT MAX(DA_HEAD_AMD_NO)
    FROM TERMS. DESPATCH_ADVICE_DETAIL
    WHERE DA_NO=ID.DISP_ADVICE_NO))UOM2,
(NVL(ID.AMOUNT,0)+NVL(ID.SGST_AMT,0) +NVL(ID.CGST_AMT,0) +NVL(ID.IGST_AMT,0)) GR_AMT,
(SELECT NAME FROM TERMS.STATE WHERE STATE_GST_CODE = VA.STATE_GST_CODE) ST_NAME,
(SELECT VMAIL FROM TERMS. UNIT WHERE CODE=IH.UNIT_CODE) WEBSITE,
(SELECT 'SALES DEPO - '||ADDRESS FROM TERMS. LOCATION L WHERE L.LOCAT_CODE = IH.LOCATION)CORP_ADD,
ID.DIS_PER,
VA.PIN_CODE CUST_PIN,
(SELECT PAN_NO FROM TERMS. VENDOR_MASTER A WHERE A.VENDOR_CODE=CONSIGNEE_CODE) CONS_PAN_NO,
(SELECT NAME FROM TERMS.STATE WHERE STATE_GST_CODE =(SELECT A.STATE_GST_CODE FROM TERMS. VENDOR_REGD_ADDRESS A
    WHERE A.VENDOR_CODE=CONSIGNEE_CODE)) CONS_ST_NAME,
CASE
WHEN (INV_HEAD_TYPE ='N' AND IH.STOCK_TYPE ='RM' )THEN 'STOCK TRANSFER - '
WHEN (INV_HEAD_TYPE ='R' AND IH.STOCK_TYPE ='RM' )THEN 'TAX INVOICE - '
WHEN (INV_HEAD_TYPE ='G' AND IH.STOCK_TYPE ='PD' )THEN 'EXPORT INVOICE - '
WHEN (INV_HEAD_TYPE ='E' AND IH.STOCK_TYPE ='PD' )THEN 'EXPORT INVOICE - '
WHEN (INV_HEAD_TYPE ='B' AND IH.STOCK_TYPE ='PD' )THEN 'BILL OF SUPPLY - '
WHEN (INV_HEAD_TYPE ='O' AND IH.STOCK_TYPE ='OT' )THEN 'TAX INVOICE - '
WHEN (INV_HEAD_TYPE ='R' AND IH.STOCK_TYPE ='PD' )THEN 'TAX INVOICE - '
WHEN (INV_HEAD_TYPE ='S' AND IH.STOCK_TYPE ='PD' )THEN 'TAX INVOICE - '
WHEN (INV_HEAD_TYPE ='X' AND IH.STOCK_TYPE ='PD' )THEN 'STOCK TRANSFER - '
WHEN (INV_HEAD_TYPE ='R' AND IH.STOCK_TYPE ='JW') THEN 'JOBWORK INVOICE - '
WHEN (INV_HEAD_TYPE ='D' AND IH.STOCK_TYPE ='OT') THEN 'TAX INVOICE - '
ELSE 'TAX INVOICE -'
END||IDENTIFIER HEAD_INV_NO,
(SELECT 'For '||NAME FROM TERMS. UNIT WHERE CODE=IH.UNIT_CODE)AUTH_SIG,
(SELECT ADDRESS FROM TERMS. UNIT WHERE CODE=IH.UNIT_CODE)HEAD_ADD,
(SELECT SUM(GR_WT) GR_WT
    FROM (SELECT SUM(NVL(CUT_WT,0)*QUANTITY) GR_WT
    FROM TERMS.PRODUCTS,TERMS.INVOICE_DETAIL
    WHERE PRODUCT_CODE=PROD_CODE
    AND PRODUCT_CODE=PROD_CODE
    AND INV_HEAD_IDENTIFIER= IH.IDENTIFIER
    UNION
    SELECT SUM(NVL(FINISH_WT,0)*QUANTITY) GR_WT FROM TERMS. PART_MASTER,TERMS.INVOICE_DETAIL
    WHERE PART_CODE=PROD_CODE
    AND PART_CODE=PROD_CODE
    AND INV_HEAD_IDENTIFIER= IH.IDENTIFIER)) GR_WT,
(SELECT SUM(NVL(FIN_WT,0)*QUANTITY) FROM TERMS. PRODUCTS,TERMS.INVOICE_DETAIL
    WHERE PRODUCT_CODE=PROD_CODE
    AND PRODUCT_CODE=PROD_CODE
    AND INV_HEAD_IDENTIFIER= IH.IDENTIFIER) NET_WT,
(SELECT ROUND(NVL(QUANTITY,1)/NVL(OUTER_STD,1),0) FROM TERMS. PRODUCTS WHERE PRODUCT_CODE=ID.PROD_CODE AND NVL(OUTER_STD,0)>0) QTY_LCD,
IH.OTHER_AMOUNT,
VA.CITY CITY_NAME,
VA.TELEX,
GRM.GST_RATE,
PROD.PRODUCT_TYP,
 IH.DRIVER_NAME,
 IH.PHONE_NO DRIVER_NO,


(SELECT SCV.DESCRIPTION
    FROM TERMS.SEC_CONTROL_VALUES SCV
    WHERE PROD.PRODUCT_TYP = SCV.CONTROL_CODE
    AND SCV.CONTROL_TYPE = 'PRODUCT_TYP') AS PRODUCT_TYP_DESC,


(SELECT DISTINCT NAME FROM TERMS. VENDOR_MASTER WHERE VENDOR_CODE=IH.TRANSPORTER_CODE) TRANSPORT_NAME,
(SELECT CITY FROM TERMS. VENDOR_REGD_ADDRESS WHERE VENDOR_CODE=CONSIGNEE_CODE) CONSIGNEE_CITY,
(SELECT LOGO_PATH FROM TERMS.UNIT WHERE CODE = IH.UNIT_CODE) LOGO_PATH,
(SELECT INSURED_BY FROM TERMS.UNIT WHERE CODE = IH.UNIT_CODE )INSURED_BY,
(SELECT POLICY_NO FROM TERMS.UNIT WHERE CODE = IH.UNIT_CODE )POLICY_NO,
(SELECT NAME FROM TERMS.CITY WHERE CODE = (SELECT CITY_CODE FROM TERMS.UNIT WHERE CODE = IH.UNIT_CODE ))UNIT_CITY_NAME,
(SELECT CIN_NO FROM TERMS.UNIT WHERE CODE = IH.UNIT_CODE )CIN_NO,
(SELECT TELEPHONE FROM TERMS.UNIT WHERE CODE = IH.UNIT_CODE )UNIT_TELEPHONE
FROM TERMS. INVOICE_HEADER IH,
     TERMS.INVOICE_DETAIL ID,
     TERMS.VENDOR_MASTER V,
    TERMS.VENDOR_REGD_ADDRESS VA,
     TERMS.MIN_MAX_MASTER M,
      TERMS.PRODUCT PROD
    ,TERMS.GST_RATE_MASTER GRM
   ,TERMS.DESPATCH_ADVICE_DETAIL DSP_D
WHERE IH.UNIT_CODE           = $P{p_unit}
  AND IH.IDENTIFIER          = $P{p_inv_no}
  AND DSP_D.DA_NO(+)         = ID.DISP_ADVICE_NO
  AND DSP_D.PRODUCT_CODE(+)  = ID.PROD_CODE
  AND DSP_D.BATCH_NO(+)      = ID.BATCH_NO
  AND ID.INV_HEAD_IDENTIFIER = IH.IDENTIFIER
  AND ID.PROD_CODE           = M.ITEM_CD(+)
  AND ID.UNIT_CD             = M.UNIT_CD(+)
  AND ID.PROD_CODE           = PROD.CODE(+)
  AND ID.GST_CODE            = GRM.GST_CODE
AND V.VENDOR_CODE            = IH.CUST_CODE
AND V.VENDOR_CODE            = VA.VENDOR_CODE(+))

SELECT GRP, STOCK_TYPE, CANCELLED, PACK_QTY, PACK_UOM, REMARKS, IRN_NO, LOCATION,
       QR_INFO, APPROVE_CODE, CUST_CITY_CODE, SHIP_MODE, INV_NO, DISTINATION,
