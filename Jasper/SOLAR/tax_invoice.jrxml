       GST_REGUNIT, BK_NAME1, BK_NAME2, ADDRESS1, ADDRESS2, ACCOUNT_NO1,
       ACCOUNT_NO2, IFSC_CODE1, CON_ST_CODE, CON_DEL_DESTINATION, CON_GST_REG_NO,
       CON_PIN, CON_NAME, CON_TELEPHONE, TP, TP1, IFSC_CODE2, EWAY_NO, EWAY_DATE,
       PIN, PT_TP, PT_DT, INV_VAL_FG, INV_VAL_WD, TOTAMT, PACK_DD, INS_TP, TYP,
       CESS_PER, CESS_AMT, CESS, INV_HEAD_TYPE, UOM2, SUM(GR_AMT)GR_AMT, ST_NAME,
       WEBSITE, CORP_ADD, DIS_PER, CUST_PIN, CONS_PAN_NO, CONS_ST_NAME,
