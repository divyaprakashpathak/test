       CONSIGNEE_ADD, SUM(QUANTITY)QUANTITY, PRICE, SUM(TAX_AMOUNT)TAX_AMOUNT,
       SUM(AMOUNT)AMOUNT, DISCOUNT, UOM, SUM(IGST_AMT)IGST_AMT, IGST_RATE,
       SUM(CGST_AMT)CGST_AMT, CGST_RATE, SUM(SGST_AMT)SGST_AMT, SGST_RATE, C_CESS,
